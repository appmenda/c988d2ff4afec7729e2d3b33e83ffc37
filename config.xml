<?xml version="1.0" encoding="UTF-8" ?>
<widget xmlns   = "http://www.w3.org/ns/widgets"
        xmlns:gap   = "http://phonegap.com/ns/1.0"
        id          = "com.fatfem20.blogspot.app"
        versionCode = "10"
        version     = "1.0.0">

    <name>FATFEM LECTURE MATERIALS </name>

    <description>
        download all lecture materials you need from here.
    </description>

    <author>Tadapp</author>

    <plugin name='cordova-plugin-splashscreen' />
    <plugin name='cordova-plugin-inappbrowser' />
    <plugin name='cordova-plugin-network-information' />

    <preference name='phonegap-version' value='cli-9.0.0' />

    <gap:plugin name='onesignal-cordova-plugin' spec='2.4.5' source='npm' />

    <preference name='android-build-tool' value='gradle' />
    <preference name="android-targetSdkVersion" value="28" />
    <preference name='orientation' value='portrait' />

    <preference name="SplashMaintainAspectRatio" value="false" />
    <preference name="SplashScreenSpinnerColor" value="#ffffff" />

    <preference name="Fullscreen" value="false" />


    <platform name="android">
        <icon src="res/android/icon/1580552497_38cef3da940b5b2cbc2e.png" density="ldpi" />
        <icon src="res/android/icon/1580552497_1a1d475a647949a3f65f.png" density="mdpi" />
        <icon src="res/android/icon/1580552497_c1128b019f75e09aff93.png" density="hdpi" />
        <icon src="res/android/icon/1580552497_d7849928fd0304a9806d.png" density="xhdpi" />
        <icon src="res/android/icon/1580552497_4c7c5ca30aef5c90b086.png" density="xxhdpi" />
        <icon src="res/android/icon/1580552497_a299f055ca454d86e118.png" density="xxxhdpi" />

        <splash src="res/android/screen/1580552375_c7166d125d916c1421f2.png" density="port-hdpi"/>
        <splash src="res/android/screen/1580552375_fc20ced4beb01643ca0d.png" density="port-ldpi"/>
        <splash src="res/android/screen/1580552375_4b53f1a81179f08160d0.png" density="port-mdpi"/>
        <splash src="res/android/screen/1580552375_f0c9a664bf1d013eda29.png" density="port-xhdpi"/>
        <splash src="res/android/screen/1580552375_f7e308b535c13c3a3fcf.png" density="land-hdpi"/>
        <splash src="res/android/screen/1580552375_73b1152eedc59328ee4b.png" density="land-ldpi"/>
        <splash src="res/android/screen/1580552375_d6e3218be74df4c531db.png" density="land-mdpi"/>
        <splash src="res/android/screen/1580552375_ba7f34ebad5c64a965a6.png" density="land-xhdpi"/>
    </platform>

    <access origin='*'/>
    <plugin name='cordova-plugin-whitelist' />
    <allow-intent href='http://*/*'/>
    <allow-intent href='https://*/*'/>
    <allow-intent href='tel:*'/>
    <allow-intent href='sms:*'/>
    <allow-intent href='mailto:*'/>
    <allow-intent href='geo:*'/>

</widget>